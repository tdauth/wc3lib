		<contexts>
			<context attribute="Normal Text" lineEndContext="#stay" name="Normal">
				<DetectSpaces />
				<keyword attribute="Keyword" context="Animatable" String="statickeyword" />
				<keyword attribute="Section" context="#stay" String="sections" />
				<keyword attribute="String Section" context="#stay" String="stringsections" />
				<keyword attribute="Long Section" context="#stay" String="longsections" />
				<keyword attribute="Long 2 Section" context="#stay" String="long2sections" />

				<keyword attribute="Property" context="#stay" String="properties" />
				<keyword attribute="Property" context="#stay" String="stringproperties" />
				<keyword attribute="Property" context="#stay" String="floatproperties" />
				<keyword attribute="Property" context="#stay" String="longproperties" />
				<keyword attribute="Property" context="ValueProperty" String="valueproperties" />

				<Float attribute="Float" context="#stay" />
				<Int attribute="Decimal" context="#stay" />
				<DetectChar attribute="String" context="String" char="&quot;"/>
				<Detect2Chars attribute="Comment" context="Commentar" char="/" char1="/" />
				<DetectChar attribute="Symbol" context="#stay" char="{" beginRegion="Brace1" />
				<DetectChar attribute="Symbol" context="#stay" char="}" endRegion="Brace1" />
			</context>

			<context attribute="Error" lineEndContext="#stay" name="Animatable">
				<DetectSpaces attribute="Normal Text" />
				<keyword attribute="Property" context="#pop" String="animatableproperties" />
			</context>

			<context attribute="Error" lineEndContext="#stay" name="ValueProperty">
				<DetectSpaces attribute="Normal Text" />
				<keyword attribute="Value" context="#pop" String="values" />
				<Int attribute="Decimal" context="#pop" /> <!-- GeosetId and GeosetAnimId -->
			</context>

			<context attribute="Region Marker" lineEndContext="#pop" name="Region Marker">
			</context>

			<context attribute="String" lineEndContext="#stay" name="String">
				<LineContinue attribute="String" context="#stay"/>
				<HlCStringChar attribute="String Char" context="#stay"/>
				<DetectChar attribute="String" context="#pop" char="&quot;"/>
			</context>

			<context attribute="Comment" lineEndContext="#pop" name="Commentar">
				<LineContinue attribute="Comment" context="#stay"/>
				<DetectSpaces />
				<DetectIdentifier />
			</context>
		</contexts>
		<itemDatas>
			<itemData name="Normal Text"            defStyleNum="dsNormal" spellChecking="false"/>
